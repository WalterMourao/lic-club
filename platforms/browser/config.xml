<?xml version='1.0' encoding='utf-8'?>
<widget id="club.licitacao" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>licitacao .club</name>
    <description>
        licitacao .club
    </description>
    <author email="suporte@licitacao.club" href="https://licitacao.club">
        Equipe licitação.club
    </author>
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-targetSdkVersion" value="21" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="load-url-timeout" value="20000" />
    <icon src="www/img/icon.png" />
    <access origin="https://licitacao.club" subdomains="true" />
    <access origin="https://wistia.com" subdomains="true" />
    <access origin="http://wistia.com" subdomains="true" />
    <allow-intent href="https://licitacao.club/*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
</widget>
