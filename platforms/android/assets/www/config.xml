<?xml version='1.0' encoding='utf-8'?>
<widget id="club.licitacao" version="3.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>licitação.club</name>
    <description>
        licitação.club
    </description>
    <author email="suporte@licitacao.club" href="https://licitacao.club">
        Equipe licitação.club
    </author>
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-targetSdkVersion" value="21" />
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="3.7.0" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="load-url-timeout" value="20000" />
    <icon src="icon.png" />
    <icon gap:density="ldpi" gap:platform="android" src="images/icon-36-ldpi.png" />
    <icon gap:density="mdpi" gap:platform="android" src="images/icon-48-mdpi.png" />
    <icon gap:density="hdpi" gap:platform="android" src="images/icon-72-hdpi.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="images/icon-96-xhdpi.png" />
    <icon gap:platform="ios" height="57" src="images/icon-57-hdpi.png" width="57" />
    <icon gap:platform="ios" height="72" src="images/icon-72-hdpi.png" width="72" />
    <gap:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
    <engine name="android" spec="^4.0.0" />
    <access origin="https://licitacao.club" subdomains="true" />
    <allow-navigation href="https://licitacao.club/*" />
</widget>
